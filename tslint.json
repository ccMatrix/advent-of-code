{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    },
    "max-classes-per-file": false,
    "quotemark": [
      true, "single", "avoid-escape"
    ],
    "no-console": false,
    "no-bitwise": false,
    "max-line-length": false
  },
  "rulesDirectory": []
}
